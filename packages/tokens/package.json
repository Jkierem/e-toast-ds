{
  "name": "@flockds/tokens",
  "version": "1.0.0",
  "description": "tokens",
  "main": "./flock.css",
  "license": "MIT",
  "files": [
    "flock.css",
    "sass"
  ],
  "dependencies": {
    "node-sass": "^4.13.1",
    "sass": "^1.26.3"
